[

  {
    "description": "模拟一个带header信息的post请求",
    "request": {
      "uri": "/post/with/headers",
      "method": "post",
      "headers": {
        "content-type": "application/json"
      },
      "json":{
        "name": "lisi",
        "sex": "woman"
      }
    },
    "response": {
      "status": 200,
      "json": {
        "lisi": "success",
        "status": 1
      }
    }
  }


]